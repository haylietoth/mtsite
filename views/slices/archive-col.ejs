<div class="archive-column">
  <% for (let i = 0; i < slice.items.length; i++) { %>
    <img
      class="archive-thumbnail"
      id="<%= slice.items[i].first.id %>" 
      src="<%= slice.items[i].first.url %>" 
      alt="<%- slice.items[i].first.alt %>">
  <% } %>
</div>
