div.whitebar
 header.site-header
  span.box
   a.underline.logo(href='./')
     .logo Madeleine Toth Edwards
  
  if menuContent != null
    nav
      ul
       li
        div.box
         a.underline.to-work(href="#" aria-label="link to homepage work section") WORK
       li
        div.box
         a.underline.to-info(href='#' aria-label="link to homepage info section") INFO
