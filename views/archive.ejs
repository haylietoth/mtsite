<!DOCTYPE html(lang='en')>
<html>
  <head>
    <%- include('./partials/head'); %>
  </head>
  <body id="archive">
    <div class="loading"></div>
    <div class="overlay">
      <div class="img-menu">
        <i id="exit" class="menu-icon material-icons">&#xE5CD;</i>
        <i id="share" class="menu-icon material-icons">&#xE5C8;</i>
        <div id="share-menu" class="share-menu">
          <a id="fb" href="" target="_blank"><i class="fa fa-facebook"></i>SHARE TO FACEBOOK</a>
          <a id="twt" href="" target="_blank"><i class="fa fa-twitter"></i>SHARE TO TWITTER</a>
          <a id="pnst" href="" target="_blank"><i class="fa fa-pinterest"></i>SHARE TO PINTEREST</a>
          <a id="dwn" download href=""><i class="fa fa-download"></i>DOWNLOAD</a>
        </div>
        <i id="zoom" class="menu-icon material-icons">&#xE8FF;</i>
        <i id="fullscreen" class="menu-icon material-icons">&#xE5D0;</i>
      </div>
    </div>
    <div class="whitebar" style="background-color: #FFF;">
    <%- include('./partials/nav', { id: 'archive' }); %>
    <%- include('./partials/scroll-btn'); %>
    <div class="archive" style="display:block;">
      <h1 class="archive-title"><%- ctx.prismicH.asText(pageContent.data.page_title) %></h1>
      <div class="archive-container">
        <%- include("SliceZone", {SliceZone: pageContent.data.body}); %>
      </div>
    </div>
    <%- include('./partials/footer'); %>
  </body>
</html>
